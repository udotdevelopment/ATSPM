@model SPM.Models.MeasuresDefaultsViewModel

<div id="wait-time-options" role="tablist" aria-multiselectable="true" class="card mr-4 ml-4">
    <div class="card-header" role="tab">
        <a onclick="rotate(this)" data-toggle="collapse" data-parent="#wait-time-options" href="#wait-time-options-collapse" aria-expanded="false" aria-controls="wait-time-optionsCollapse">
            <i class="fa fa-chevron-right mr-3" aria-hidden="true"></i> Wait Time
        </a>
    </div>
    <div id="wait-time-options-collapse" class="collapse" role="tabpanel">
        <div class="card-body">
            <div class="row p-1">
                <div class="col-9">
                    @Html.LabelFor(model => model.WaitTime.ShowPlanStripes, htmlAttributes: new { @class = "control-label" })
                </div>
                <div class="col-3 text-right">
                    @Html.EditorFor(model => model.WaitTime.ShowPlanStripes)
                    @Html.ValidationMessageFor(model => model.WaitTime.ShowPlanStripes, "", new { id = "Description-error", @class = "text-danger" })
                </div>
            </div>
        </div>
    </div>
</div>

